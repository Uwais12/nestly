{
  "expo": {
    "name": "nestly",
    "slug": "nestly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/NestlyAppIcons/light/icon_1024x1024.png",
    "scheme": "nestly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nestly.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.nestly.app",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/NestlyAppIcons/light/icon_180x180.png",
        "backgroundImage": "./assets/images/NestlyAppIcons/light/icon_167x167.png",
        "monochromeImage": "./assets/images/NestlyAppIcons/dark/icon_40x40.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/NestlyAppIcons/light/icon_180x180.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-share-intent",
        {
          "iosExtensionName": "ShareNestly",
          "iosActivationRule": "TRUEPREDICATE",
          "androidMimeTypes": [
            "text/plain",
            "text/uri-list"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64dfb0b9-1934-4018-8fe6-f78e19b8ca29"
      }
    }
  }
}
